<%= form_tag '/logins', :class => 'form' do %>

	<div>
		<%= label_tag :email %><br />
		<%= email_field_tag :email,:required => true %>
	</div>

	<div>
		<%= label_tag :password %> <br />
		<%= password_field_tag :password,:required => true%>
	</div>

	<%= submit_tag "Submit", class: "login-submit" %> 
<% end %>
